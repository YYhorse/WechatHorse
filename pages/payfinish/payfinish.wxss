page {  
  display: block;  
  min-height: 100%;  
  background-color: #F8F8F8;  
}  
.BuyCarItemLogo{
  width:30rpx;
  height:30rpx;
}
.OrderCodeTitle{
  display: flex;
  width: 100%;
  height: 50rpx;
  flex-direction: row;
  align-items: center;
  background-color: #ededed;
}
.OrderCodeTxt{
  font-size: 25rpx;
  margin-left: 5rpx;
}
.linerowngray{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  width: 2rpx;
  background-color: #ededed
}
.linecolumngray{
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-bottom: 1px solid #ededed;
}
.TakeMealNumberLayout{
  display: flex;
  width: 100%;
  height: 200rpx;
  flex-direction: row;
}
.TakeMealQueueLayout{
  display: flex;
  width: 50%;
  flex-direction: column;
  align-items: center;
}
.TakeMealQueueTxt{
  font-size: 25rpx;
  color: black;
  margin-top: 20rpx;
}
.TakeMealQueueNumber{
  font-size: 80rpx;
  font-weight:bold;
  color: #00CD00;
  margin-top: 10rpx;
}
.TakeMealDeskLayout{
  display: flex;
  width: 50%;
  flex-direction: column;
  align-items: center;
}
.TakeMealDeskName{
  font-weight:bold;
  color: #1C86EE;
  margin-top: 10rpx;
}
.MenuTitle{
  display: flex;
  width: 100%;
  height: 50rpx;
  flex-direction: row;
  align-items: center;
  background-color: #ededed;
}
.MenuTitle2{
  display: flex;
  width: 100%;
  height: 80rpx;
  flex-direction: row;
  align-items: center;
  background-color: #ededed;
}
.ActualTxt{
  font-size: 35rpx;
  margin-left: 5rpx;
}
.MenuLayout{
  display: flex;
  width: 100%;
  flex-direction: column;
}
.MenuItemLayout{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 60rpx;
  padding: 5px 0;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-bottom: 1px solid #ededed;
}
.MenuItemPhoto{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.MenuItemName{
  font-size: 30rpx;
  color: black;
  margin-left: 30rpx;
  width: 350rpx;
  overflow:hidden;      
  text-overflow:ellipsis;
  white-space:nowrap;  
}
.MenuItemNumber{
  width: 250rpx;
  margin-left: 10rpx;
  font-size: 30rpx;
  color:gray;
}
.MenuItemPrice{
  width: 150rpx;
  margin-left: 10rpx;
  font-size: 30rpx;
  color: #FF6101;
}
.ActivityImage{
  width:80rpx;
  height:60rpx;
  margin-left: 50rpx;
  margin-top: 20rpx;
}
.ActivityInfoContextLayout{
  align-items: center;
  display: flex;
  width: 80%;
  margin-top: 20rpx;
  flex-direction: row-reverse;
}
.ActivityInfoContext{
  font-size: 30rpx;
  color: #FF6101;
  font-weight:bold;
  margin-right: 20rpx;
}
.ActivityInfoLayout{
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.ActivityInfoImage{
  width:60rpx;
  height:60rpx;
  margin-left: 50rpx;
}
.ActivityInfoGiftImage{
  width:60rpx;
  height:60rpx;
  margin-left: 50rpx;
}
.ActivityInfoContextLayout{
  align-items: center;
  display: flex;
  width: 80%;
  margin-top: 20rpx;
  flex-direction: row-reverse;
}
.ActivityInfoContext{
  font-size: 30rpx;
  color: #FF6101;
  font-weight:bold;
  margin-right: 20rpx;
}
.OneRowTwoDataLayout{
  width: 100%;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.FirstDataTxt{
  font-size: 25rpx;
  color: gray;
  margin-left: 30rpx;
}
.SecondDataLayout{
  align-items: center;
  display: flex;
  width: 80%;
  flex-direction: row-reverse;
}
.SecondDataLayout2{
  align-items: center;
  display: flex;
  width: 80%;
  flex-direction: row-reverse;
}
.SecondDataTxt{
  font-size: 28rpx;
  color: gray;
  margin-right: 20rpx;
}
.FirstData2Txt{
  font-size: 40rpx;
  color: gray;
